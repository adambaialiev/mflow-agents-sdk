{
  "name": "mflow-agents-sdk",
  "version": "0.1.3",
  "description": "SDK for creating agents on mflow platform",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adambaialiev/mflow-agents-sdk.git"
  },
  "homepage": "https://github.com/adambaialiev/mflow-agents-sdk#readme",
  "bugs": {
    "url": "https://github.com/adambaialiev/mflow-agents-sdk/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:types": "tsc --project tsconfig.types.json",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mflow",
    "agents",
    "sdk",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.83.0",
    "@anthropic-ai/sdk": "^0.39.0",
    "express": "^4.19.2",
    "together-ai": "^0.13.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "jest": "^29.5.0",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
